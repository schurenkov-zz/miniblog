{
    "compilerOptions": {
      "sourceMap": true,
      "module": "commonjs",
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "experimentalDecorators": true,
      "target": "es5",
      "jsx": "react",
      "lib": ["dom", "es6", "es2017"],
      "locale": "ru",
      "baseUrl": ".",
      "paths": {
        "@STORE/*": ["./src/store/*"]
      }
    },
    "include": ["src"],
    "exclude": ["node_modules"],
    "compileOnSave": false
  }
